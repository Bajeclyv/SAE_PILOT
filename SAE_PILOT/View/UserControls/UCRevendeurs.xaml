<UserControl
    x:Class="SAE_PILOT.View.UserControls.UCRevendeurs"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SAE_PILOT.View.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <DataGrid HorizontalAlignment="Center">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Raison sociale" />
                <DataGridTextColumn Header="Adresse" />
            </DataGrid.Columns>
        </DataGrid>
        <Button
            Width="200"
            Height="40"
            Content="Ajouter un revendeur"
            FontSize="16" />
    </StackPanel>
</UserControl>
