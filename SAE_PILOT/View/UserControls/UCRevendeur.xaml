<UserControl
    x:Class="SAE_PILOT.View.UserControls.UCRevendeur"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SAE_PILOT.View.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="550     "
    mc:Ignorable="d">
    <Grid>
        <Label
            x:Name="labActionRevendeur"
            Margin="102,22,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Que souhaitez vous faire ?"
            FontFamily="Rubik"
            FontSize="26"
            FontWeight="Bold"
            Foreground="#FF19459A" />
        <Button
            x:Name="butCreerRevendeur"
            Width="36"
            Margin="114,406,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="#FF19459A"
            BorderBrush="{x:Null}"
            Content="&#xE710;"
            FontFamily="Segoe MDL2 Assets"
            Foreground="#FFF8F6F6"
            ToolTip="Ajouter" />
        <Button
            x:Name="butModifierRevendeur"
            Width="54"
            Margin="0,406,0,0"
            HorizontalAlignment="Center"
            VerticalAlignment="Top"
            Background="#FF19459A"
            BorderBrush="{x:Null}"
            Content="&#xE70F;"
            FontFamily="Segoe MDL2 Assets"
            Foreground="#FFF8F6F6"
            ToolTip="Modifier" />
        <Button
            x:Name="butSupprimerRevendeur"
            Width="67"
            Margin="384,406,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="#FF19459A"
            BorderBrush="{x:Null}"
            Content="&#xE74D;"
            FontFamily="Segoe MDL2 Assets"
            Foreground="#FFF8F6F6"
            ToolTip="Supprimer" />

    </Grid>
</UserControl>
